{
    "plugins": [
        "prettier",
        "unicorn"
      ],
      "extends": [
        "plugin:prettier/recommended",
        "prettier"
      ],
    "env": {
      "es6": true,
      "browser": true
    },
    "rules": {
        "unicorn/prevent-abbreviations": "off",
        "prettier/prettier": "error", 
        "endOfLine": "auto"
    }
  }